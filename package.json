{
  "name": "deip-operations-examples",
  "version": "1.0.0",
  "description": "This is a sample of DEIP protocol operations",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && babel ./src --out-dir=./dist --config-file=./babel.config.js --copy-files --include-dotfiles",
    "start:substrate-rpc": "npm run build && node dist/substrate-rpc/index.js",
    "start:casimir": "npm run build && node dist/casimir/index.js",
    "start:casimir-nft": "npm run build && node dist/casimir-nowar-nft-flow/index.js",
    "start:casimir-nft-lazy": "npm run build && node dist/casimir-nowar-nft-flow-lazy/index.js",
    "start:ft-nft-test": "npm run build && node dist/casimir-ft-nft-test/index.js",
    "start:ft-nft-test-2-proposals": "npm run build && node dist/casimir-ft-nft-test/index.js",
    "start:generate-portal-config": "npm run build && node dist/generate-portal-conifg/index.js",
    "start:casimir-database-setup": "npm run build && node dist/casimir-database-setup/index.js",
    "start:casimir-nowar-nft-flow-lazy-2-proposals": "npm run build && node dist/casimir-nowar-nft-flow-lazy-2-proposals/index.js",
    "start:casimir-init": "NODE_ENV_PATH=./../../src/.config.env npm run start:generate-portal-config && npm run start:casimir-database-setup",
    "checkUpdateDeip": "npx ncu \"/^@deip\\/.*$/\"",
    "linkModules": "node ./scripts/linkDevModules.js",
    "updateDeip": "npx ncu \"/^@deip\\/.*$/\" -u"
  },
  "author": "DEIP",
  "license": "ISC",
  "dependencies": {
    "@deip/chain-service": "1.382.2",
    "@deip/commands": "1.382.2",
    "@deip/toolbox": "1.382.2",
    "@polkadot/api": "8.6.2",
    "@polkadot/rpc-provider": "8.6.2",
    "@polkadot/types": "8.6.2",
    "@polkadot/util-crypto": "^9.0.1",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "mongodb": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.0",
    "@babel/register": "^7.16.0",
    "fs-extra": "^10.0.1",
    "glob": "^7.2.0",
    "inquirer": "^8.2.0",
    "npm-check-updates": "^12.0.2",
    "rimraf": "^3.0.2",
    "symlink-dir": "^5.0.1"
  }
}
